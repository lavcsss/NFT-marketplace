<main class="main m_home">

  <%= render "common/home_header"%>
  <section class="blog-section">
    <div class="container">
        <div class="row ">
            <div>
                <div class="main__title">
                    <div class="main__title"><h2><a> Notable Drops</a></h2></div>
                </div>
            </div>
            <div class="row d-flex justify-content-center align-items-center">
                <div class="col-lg-12 flex-column col-12 col-md-12 d-flex justify-content-center align-items-center">
                    <div class="row" style="width:80%;justify-content:space-around">
                        <div class="col-md-4 col-sm-12 mt-3" style="max-width:315px">
                            <div class="row row--grid">
                                <div class="li-first-range all-out-one">
                                    <div class="blank-img cursor" href="/nft/coming-soon.html">
                                        <img alt="img" class="img-fluid" height="auto" src="https://crosstower-india-prod.s3.ap-south-1.amazonaws.com/notable1.png" width="auto"/>
                                    </div>
                                    <div class="pre-heading"><h4>Antara Movie NFT's</h4></div>
                                    <div class="pre-heading" style="min-height:60px"><h4>The History Making NFT</h4>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-12 mt-3" style="max-width:315px">
                            <div class="row row--grid">
                                <div class="li-first-range all-out-one">
                                    <div class="blank-img cursor" href="/nft/coming-soon.html" style="max-height:450px">
                                        <img alt="img" class="img-fluid" height="auto" src="https://crosstower-india-prod.s3.ap-south-1.amazonaws.com/notable2.png" width="auto" />
                                    </div>
                                    <div class="pre-heading"><h4>Shannon Rose Studio NFT's</h4></div>
                                    <div class="pre-heading" style="min-height:60px"><h4>Exclusive Paintings and Mixed
                                        Media Artworks</h4></div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-12 mt-3" style="max-width:315px">
                            <div class="row row--grid">
                                <div class="li-first-range all-out-one">
                                    <div class="blank-img cursor" href="/nft/coming-soon.html">
                                        <img alt="img" class="img-fluid" height="auto" width="auto" src="https://crosstower-india-prod.s3.ap-south-1.amazonaws.com/mike-notable.png"/>
                                    </div>
                                    <div class="pre-heading"><h4>Mike Boornazian NFT's</h4></div>
                                    <div class="pre-heading" style="min-height:60px"><h4>An American Actor, Model and
                                        Influencer</h4></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
  </section>
  <div class="container">
    <div class="row row--grid">
      <div class="col-12">
        <div class="main__title">
          <h2><a href="#">Live auctions</a></h2>
        </div>
      </div>
      <div class="col-12">
      
      <% if @hot_bids.present? %>
        <div class="main__carousel-wrap">
          <div class="main__carousel main__carousel--live owl-carousel" id="live">
            
            <% @hot_bids.each do |collection|%>
              <%= render "hot_bids", collection: collection %>
            <%end%>
            
          </div>
          <div class="one-elements">
            <a href="<%= search_path%>" class="explore-all">Explore all</a>
            <button class="main__nav main__nav--prev" data-nav="#live" type="button"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M17,11H9.41l3.3-3.29a1,1,0,1,0-1.42-1.42l-5,5a1,1,0,0,0-.21.33,1,1,0,0,0,0,.76,1,1,0,0,0,.21.33l5,5a1,1,0,0,0,1.42,0,1,1,0,0,0,0-1.42L9.41,13H17a1,1,0,0,0,0-2Z"/></svg></button>
            <button class="main__nav main__nav--next" data-nav="#live" type="button"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M17.92,11.62a1,1,0,0,0-.21-.33l-5-5a1,1,0,0,0-1.42,1.42L14.59,11H7a1,1,0,0,0,0,2h7.59l-3.3,3.29a1,1,0,0,0,0,1.42,1,1,0,0,0,1.42,0l5-5a1,1,0,0,0,.21-.33A1,1,0,0,0,17.92,11.62Z"/></svg></button>
          </div>
        </div>
      <% else %>
        <%= render partial: 'common/empty' %>
      <% end %>

      </div>
    </div>

    <div class="row row--grid">
      <!-- title -->
      <div class="col-12">
        <div class="main__title">
          <h2><a href="/search"><%= t('dashboard.explore')%></a></h2>
        </div>
      </div>
      <!-- end title -->
      <div class="col-12 filters-content">
        <div class="row row--grid">
          <% if @category_collections.present? %>
            <% @category_collections.each do |collection| %>
              <div class="col-12 col-sm-6 col-lg-4 col-xl-3">
                <%= render "dashboard/collection_card", collection: collection %>
              </div>
            <% end %>
          <% else %>
            <% 1.times do %>
              <div class="col-12">
                <%= render partial: 'common/empty' %>
              </div>
            <% end %>
          <% end %>
        </div>
      </div>
    </div>

    <div class="col-12">
      <% if params[:page_no].to_i < @category_collections.total_pages %>
        <div class="row">
          <div class="col-md-12 pt-15 pb-15">
            <div class="load-more-items">
              <a href="javascript:void(0)" data-page-no="2" category="" class="dashboard-load-more">Load more items</a>
            </div>
          </div>
        </div>
      <% end %>
    </div>


