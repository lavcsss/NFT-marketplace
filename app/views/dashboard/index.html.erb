          </ul>
        </div>

      </div>
  </section>
  
  <% if @featured_users.present? %>
    <div class="row row--grid">
      <div class="col-12">
        <div class="main__title">
          <h2><a href="javascript:void(0);"><%= t('dashboard.featured_users')%></a></h2>
        </div>
      </div>
      <div class="col-12">
        <div class="featureLayer owl-carousel">
          <% @featured_users.each do |featured_user| %>
            <div class="item">
              <%= render "dashboard/featured_user_card", featured_user: featured_user %>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  <% end %>

    <section class="one-youtube">
